# Test configuration file

apply testApply test:apply test:applyC Test apply record

command test testApply Basic test command
{
    param1 test:test.param1 First parameter
    param2 test:test.param2 Second parameter
}

command init testApply Init command
{
    dir test:init.DIR
}

command reboot testApply Reboot command
{
    dummy test:reboot.DIR
}

status testStatus Test status
{
    intStatus test:intVal 0 Integer status
    strStatus test:strVal 0 String status
    dblStatus test:dblVal 0 Double status
    fltStatus test:fltVal 0 Float status 
}
