<?xml version="1.0" encoding="UTF-8"?>
<tns:Records xmlns:tns="http://www.gemini.edu/CaSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.gemini.edu/CaSchema ../../main/resources/CaSchema.xsd">
	<tns:Top name="test">test</tns:Top>
	<tns:Apply name="testApply">
		<tns:top>test</tns:top>
		<tns:apply>apply</tns:apply>
		<tns:car>applyC</tns:car>
		<tns:description>Test apply record</tns:description>
		<tns:command name="test">
			<tns:description>Basic test command</tns:description>
			<tns:parameter name="param1">
				<tns:channel>test.param1</tns:channel>
				<tns:type>STRING</tns:type>
				<tns:description>First parameter</tns:description>
			</tns:parameter>
			<tns:parameter name="param2">
				<tns:channel>test.param2</tns:channel>
				<tns:type>STRING</tns:type>
				<tns:description>Second parameter</tns:description>
			</tns:parameter>
		</tns:command>
		<tns:command name="reboot">
			<tns:record>reboot</tns:record>
			<tns:description>Reboot command</tns:description>
		</tns:command>
		<tns:command name="init">
			<tns:record>init</tns:record>
			<tns:description>Init command</tns:description>
		</tns:command>
	</tns:Apply>
	<tns:Status name="testStatus">
		<tns:top>test</tns:top>
		<tns:description>Test status</tns:description>
		<tns:attribute name="intStatus">
			<tns:channel>intVal</tns:channel>
			<tns:type>INTEGER</tns:type>
			<tns:description>Integer status</tns:description>
		</tns:attribute>
		<tns:attribute name="strStatus">
			<tns:channel>strVal</tns:channel>
			<tns:type>STRING</tns:type>
			<tns:description>String status</tns:description>
		</tns:attribute>
		<tns:attribute name="dblStatus">
			<tns:channel>dblVal</tns:channel>
			<tns:type>DOUBLE</tns:type>
			<tns:description>Double status</tns:description>
		</tns:attribute>
		<tns:attribute name="fltStatus">
			<tns:channel>fltVal</tns:channel>
			<tns:type>FLOAT</tns:type>
			<tns:description>Float status</tns:description>
		</tns:attribute>
	</tns:Status>
</tns:Records>
